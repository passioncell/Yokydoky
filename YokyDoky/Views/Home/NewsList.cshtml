@model IEnumerable<Business.Entitys.ArticleListModel>

@{
    ViewBag.Title = "NewsList";
}

<br />
<ul class="nav nav-pills" role="tablist">
    <li role="presentation" class="active"><a href="#">전체<span class="badge">42</span></a></li>
    <li role="presentation"><a href="#">문화</a></li>
    <li role="presentation"><a href="#">경제</a></li>
    <li role="presentation"><a href="#">국제</a></li>
    <li role="presentation"><a href="#">과학/IT</a></li>
    <li role="presentation"><a href="#">정치</a></li>
    <li role="presentation"><a href="#">사회</a></li>
</ul>

<br />
@foreach (var item in Model)
{
    <div class="panel panel-info">
        <div class="panel-heading"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> @Html.DisplayFor(modelItem => item.title)</div>
        <div class="panel-body">
            <p>
              리포터 : @Html.DisplayFor(modelItem => item.reporter) <br />
              작성일시 : @Html.DisplayFor(modelItem => item.write_date)<br />
              수정일시 : @Html.DisplayFor(modelItem => item.modify_date)<br />
              키워드 : @Html.DisplayFor(modelItem => item.keyword)
            </p>
            @Html.DisplayFor(modelItem => item.content)
        </div>
    </div>

}
